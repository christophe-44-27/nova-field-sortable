{
    "name": "christophe-44-27/nova-field-sortable",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.3",
        "laravel/nova": "^5.0",
        "spatie/eloquent-sortable": "^4.5"
    },
    "autoload": {
        "psr-4": {
            "CodeWithChristophe\\NovaFieldSortable\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeWithChristophe\\NovaFieldSortable\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
